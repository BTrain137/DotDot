# ~/.ssh/config

# Default github account
Host github.com
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_rsa_github

# Work github account
Host github.com-work
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_rsa_github_zymoreseaerch

# Example to clone personal 
# git clone git@github.com-work:bryan89tran/ideal-octo-couscous.git

# Visual Studio Team services
Host vs-ssh.visualstudio.com
  HostName vs-ssh.visualstudio.com # Must include sub domain
  User btran # Sign in username
  IdentityFile ~/.ssh/id_rsa_visualstudio_team

# git clone btran@vs-ssh.visualstudio.com:v3/zymoresearch/Web_Apps/internal_ordering
